package collections;

public class A {

	public static void main(String[] args) {
		int arr[] = {999294254,998517730,998227827,997041077,995945427,995000092,994947496,994498231,990679587,990362048,990002876,989999950,986040899,985248887,985023829,983478322,980695499,979681953,977958945,973316409,973067911,970916525,970423348,965005484,964959823,964616884,964587195,963645349,962726787,962525810,961874593,961772991,961650158,961251504,960931476,959934657,959694853,959350299,957076503,956943627,955513534,955070517,954208471,953165280,951927392,951196202,950350381,949895394,948938193,948177067,948007289,947940342,947513064,945311931,945144262,944876585,943469484,942975531,940459934,940096782,938352890,937443620,936149401,935983516,934409645,932269515,931992530,931901388,928436554,927272311,926241879,925867643,923658777,921888761,921746721,920947984,919225452,916730226,912842481,909061346,908672404,908169737,907869186,904354885,904098206,903863836,902623435,900555892,897753211,896504372,895832255,893553939,893341935,892957729,892876128,891564530,888795724,887603710,887293257,884589475};
		System.out.println("Maximum difference is " + maxDiff(arr));
	}

	public static int maxDiff(int[] nums) {
		int mn = nums[0], res = -1;
	    for (int i = 1; i < nums.length; i++) {
	        res = Math.max(res, nums[i] - mn);
	        mn = Math.min(mn, nums[i]);
	    }
	    return res == 0 ? -1 : res;
	}

}
