package mainApp;

public class TestPolymorphism {  
	public static void main(String args[]){  
		Bank b;  
		b=new SBI(); 
		b.getRateOfInterest();
		b=new ICICI();  
		b=new AXIS();  
		
		AXIS a = new AXIS();
		a.getRateOfInterest();
		}  
}

class Animal{  
Animal(){System.out.println("animal is created");}  
}  
class Dog extends Animal{  
Dog(){  
super();
System.out.println("dog is created");  
}  
}  
class TestSuper4{  
public static void main(String args[]){  
Dog d=new Dog();  
}}  

